doctype html
html
  head
    title= "Video Framework"
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    div(class="navbarDiv")
      nav(class="navbar")
        a(class="brand" , href='/')
          img(class="icon",src='HomeIcon.png' )
        div(class="list")
          ul(class="navbar-scroll")
            li(class="row")
              a(class="navbarLink",href="/videotest" ) Video
        form(class="searchForm")
          input(class="searchBox",type="text",placeholder="Search")
          input(class="searchIcon",type="submit",src="SearchIcon.png", value="")
    block content
